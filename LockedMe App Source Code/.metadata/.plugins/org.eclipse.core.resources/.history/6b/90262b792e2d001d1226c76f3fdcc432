package menu;

public class MenuBuilder {
	
	public static String CurrentMenu;
	public static  Menu getMenu(int menuOption)
	{
		Menu menu;
		switch(menuOption)
		{
		case 0:
			menu = new WelcomeScreenMenu();
			return menu;
			
			
		}
		return new WelcomeScreenMenu();
		
	}
	
	public static void setCurrentMenu(String m)
	{
		CurrentMenu = m;
	}

}
